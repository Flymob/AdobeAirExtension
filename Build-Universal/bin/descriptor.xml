<?xml version="1.0" encoding="utf-8" standalone="no"?>
<extension xmlns="http://ns.adobe.com/air/extension/21.0">
	<id>flymob.com</id>
	<versionNumber>1.0.0</versionNumber>
	<platforms>

		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libFlyMobANE.a</nativeLibrary>
				<initializer>FlyMobANEExtInitializer</initializer>
				<finalizer>FlyMobANEExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>ANEFlyMobSdk.jar</nativeLibrary>
				<initializer>com.flymob.sdk.air.ANEFlyMobExtension</initializer>
				<finalizer>com.flymob.sdk.air.ANEFlyMobExtension</finalizer>
			</applicationDeployment>
	    </platform>

	     <platform name="Android-x86"> 
            <applicationDeployment>
				<nativeLibrary>ANEFlyMobSdk.jar</nativeLibrary>
				<initializer>com.flymob.sdk.air.ANEFlyMobExtension</initializer>
				<finalizer>com.flymob.sdk.air.ANEFlyMobExtension</finalizer>
			</applicationDeployment>
        </platform>  

		<platform name="default">
            <applicationDeployment/>
        </platform>

	</platforms>
</extension>